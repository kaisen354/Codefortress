{
  "name": "codeforces-viewer",
  "displayName": "Codeforces Problem Viewer",
  "description": "Displays Codeforces problems from a WebSocket connection.",
  "private": true,
  "version": "0.0.1",
 
  "engines": {
    "vscode": "^1.82.0"
  },
  "activationEvents": [
    "onCommand:codeforces-problem-viewer.showProblem"
  ],
  "main": "./extension.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "vscode:prepublish": "npm run compile",
    "compile": "npm run build-extension",
    "watch": "npm run build-extension -- --watch",
    "build-extension": "webpack --mode development"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@tailwindcss/vite": "^4.1.13",
    "dotenv": "^17.2.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.13",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "vite": "^7.1.7",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "contributes": {
    "commands": [
      {
        "command": "codeforces-problem-viewer.showProblem",
        "title": "Show Codeforces Problem"
      }
    ]
  }
}